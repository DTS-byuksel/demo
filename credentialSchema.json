{
  "name": "credentialSchema ",
  "description": "Simple representation of an credentialSchema.",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.com/certificates/JsonSchema2023.json",
  "type": "object",
  "properties": {
    "credentialSubject": {
      "type": "object",
      "properties": {
        "id": {
          "title": "Credential subject id",
          "type": "string",
          "description": "Credential subject identifier"
        },
        "attributes": {
          "title": "Attributes",
          "description": "Credential attributes",
          "type": "object",
          "properties": {
            "attributeatitle": {
              "title": "attribute a title",
              "type": "string",
              "description": "attrubite a description"
            },
            "attributebtitle": {
              "title": "attribute b title",
              "type": "integer",
              "description": "attrubite b description"
            },
            "attributectitle": {
              "title": "attribute c title",
              "type": "object",
              "description": "attrubite c description",
              "properties": {
                "type": {
                  "type": "string",
                  "const": "date-time"
                },
                "timestamp": {
                  "type": "string",
                  "format": "date-time"
                }
              },
              "required": [
                "type",
                "timestamp"
              ]
            }
          },
          "required": [
            "attributeatitle"
          ]
        }
      },
      "required": [
        "id",
        "attributes"
      ]
    }
  },
  "required": [
    "credentialSubject"
  ]
}